 ##
 #
 # AppServer specific properties
 #
[AppServer]
    allowRuntimeUpdates=0
    applications=SportsDemo
    collectMetrics=1
    statusEnabled=1

 #
 #  Default properties for Session Manager
 #
[AppServer.SessMgr]
    agentExecFile=${psc.as.oe.dlc}/bin/_mproapsv
    agentListenerTimeout=10000
    agentLogEntryTypes=
    agentLogFile=
    agentLoggingLevel=2
    agentStartLimit=1
    agentStartupParam=-T "${catalina.base}/temp"
    agentWatchdogTimeout=3000
    connectionWaitTimeout=3000
    idleAgentTimeout=300000
    idleConnectionTimeout=300000
    idleResourceTimeout=0
    idleSessionTimeout=300000
    ipver=IPv4
    maxABLSessionsPerAgent=200
    maxAgents=10
    maxConnectionsPerAgent=16
    minAgents=0
    numInitialAgents=1
    publishDir=${catalina.base}/openedge
    requestWaitTimeout=15000
    socketTimeout=3000

[AppServer.Agent]
    agentMaxPort=62202
    agentMinPort=62002
    agentShutdownProc=
    agentStartupProc=
    agentStartupProcParam=
    binaryUploadMaxSize=0
    collectStatsData=0
    fileUploadDirectory=
    flushStatsData=0
    infoVersion=9010
    keyAlias=
    keyAliasPasswd=
    # keyStorePasswd=
    keyStorePath=${DLC}/keys/
    lockAllExtLib=
    lockAllNonThreadSafeExtLib=
    noSessionCache=0
    numInitialSessions=5
    PROPATH=${CATALINA_BASE}/openedge,${DLC}/tty,${DLC}/tty/netlib/OpenEdge.Net.pl
    sessionActivateProc=
    sessionConnectProc=
    sessionDeactivateProc=
    sessionDisconnProc=
    sessionExecutionTimeLimit=0
    sessionShutdownProc=
    sessionStartupProc=
    sessionStartupProcParam=
    sessionTimeout=180
    sslAlgorithms=
    usingThreadSafeExtLib=
    uuid=
    workDir=${CATALINA_BASE}/work

[SportsDemo]
    webApps=ROOT,sports

[SportsDemo.ROOT]
    allowRuntimeUpdates=0
    collectMetrics=1
    statusEnabled=1

[AppServer.Agent.SportsDemo]
    numInitialSessions=2
    PROPATH=${CATALINA_BASE}/webapps/sports/WEB-INF/openedge,${CATALINA_BASE}/openedge,${CATALINA_BASE}/openedge/Spark.pl,${CATALINA_BASE}/openedge/Ccs.pl,${DLC}/tty,${DLC}/tty/netlib/OpenEdge.Net.pl
    sessionShutdownProc=Sports/shutdown.r
    sessionStartupProc=Spark/startup.r
    uuid=http://PACEVICIUSP:8831/SportsDemo

[SportsDemo.ROOT.APSV]
    adapterEnabled=1
    enableRequestChunking=1
    useHTTPSessions=1

[SportsDemo.ROOT.SOAP]
    adapterEnabled=1
    adminEnabled=1
    adminSoapAction=urn:services-progress-com:wsa-admin:01
    debugClients=
    wsaUrl=http://${psc.as.host.name}:${psc.as.http.port}/soap
    wsdlEnabled=1

[SportsDemo.ROOT.REST]
    adapterEnabled=1

[SportsDemo.ROOT.WEB]
    adapterEnabled=1
    defaultCookieDomain=
    defaultCookiePath=
    defaultHandler=OpenEdge.Web.CompatibilityHandler
    srvrAppMode=development
    srvrDebug=1
    wsRoot=/static/webspeed

[AppServer.SessMgr.SportsDemo]
    agentLogEntryTypes=ASPlumbing,DB.Connects
    agentLogFile=${catalina.base}/logs/SportsDemo.agent.log
    agentStartupParam=-T ${catalina.base}/temp -pf ${catalina.base}/openedge/startup.pf

[SportsDemo.sports]
    statusEnabled=1

# See $CATALINA_HOME/conf/openedge.properties.README for details on the properties below.
# DO NOT MODIFY any ${} tags
# Transport properties for the APSV protocol
[SportsDemo.sports.APSV]
    adapterEnabled=1
    enableRequestChunking=1
    useHTTPSessions=1

# Transport properties for the SOAP protocol
[SportsDemo.sports.SOAP]
    adapterEnabled=0
    adminEnabled=1
    adminSoapAction=urn:services-progress-com:wsa-admin:01
    debugClients=
    wsaUrl=http://${psc.as.host.name}:${psc.as.http.port}/sports/soap
    wsdlEnabled=1

# Transport properties for the REST protocol
[SportsDemo.sports.REST]
    adapterEnabled=1

# Transport properties for the WEB protocol
[SportsDemo.sports.WEB]
    adapterEnabled=1
    defaultCookieDomain=
    defaultCookiePath=
    defaultHandler=OpenEdge.Web.DefaultWebHandler
    handler1=OpenEdge.Web.DataObject.DataObjectHandler: /pdo/
    handler2=OpenEdge.Web.PingWebHandler: /_oeping
    handler3=Spark.Core.Handler.FileUpload: /upload
    srvrAppMode=development
    srvrDebug=0
    wsRoot=/sports/static/webspeed
